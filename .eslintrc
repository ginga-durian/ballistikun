{
    "extends": ["eslint:recommended"],
    "plugins": ["react"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        
    },
    "rules": {
        "react/jsx-uses-vars": 1
    }
}